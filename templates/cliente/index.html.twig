{% extends 'base.html.twig' %}

{% block title %}Cliente index{% endblock %}

{% block body %}
    <h1>Cliente index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Empresa</th>
                <th>Telefono</th>
                <th>Nombre</th>
                <th>Cod_cliente</th>
                <th>Direccion</th>
                <th>Correo</th>
                <th>Provincia</th>
                <th>Ciudad</th>
                <th>Contrasena</th>
                <th>Fecha</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for cliente in clientes %}
            <tr>
                <td>{{ cliente.id }}</td>
                <td>{{ cliente.empresa }}</td>
                <td>{{ cliente.telefono }}</td>
                <td>{{ cliente.nombre }}</td>
                <td>{{ cliente.codCliente }}</td>
                <td>{{ cliente.direccion }}</td>
                <td>{{ cliente.correo }}</td>
                <td>{{ cliente.provincia }}</td>
                <td>{{ cliente.ciudad }}</td>
                <td>{{ cliente.contrasena }}</td>
                <td>{{ cliente.fecha ? cliente.fecha|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('cliente_show', {'id': cliente.id}) }}">show</a>
                    <a href="{{ path('cliente_edit', {'id': cliente.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="12">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('cliente_new') }}">Create new</a>
{% endblock %}
