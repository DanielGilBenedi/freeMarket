{% extends 'base.html.twig' %}

{% block title %}Administrador index{% endblock %}

{% block body %}
    <h1>Administrador index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Correo</th>
                <th>Contrasena</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for administrador in administradors %}
            <tr>
                <td>{{ administrador.id }}</td>
                <td>{{ administrador.nombre }}</td>
                <td>{{ administrador.correo }}</td>
                <td>{{ administrador.contrasena }}</td>
                <td>
                    <a href="{{ path('administrador_show', {'id': administrador.id}) }}">show</a>
                    <a href="{{ path('administrador_edit', {'id': administrador.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('administrador_new') }}">Create new</a>
{% endblock %}
